{
  "name": "switchboard-v2-examples",
  "version": "1.0.0",
  "description": "Sample code for Switchboard V2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npm-run-all -c create airdrop build deploy",
    "create": "run-s create:*",
    "create:authority": "solana-keygen new -s --no-bip39-passphrase --outfile keypairs/authority-keypair.json",
    "create:onchain": "solana-keygen new -s --no-bip39-passphrase --outfile keypairs/on_chain_feed_parser-keypair.json",
    "create:anchor": "solana-keygen new -s --no-bip39-passphrase --outfile keypairs/anchor_feed_parser-keypair.json",
    "airdrop": "run-s airdrop:*",
    "airdrop:authority": "solana airdrop 5 keypairs/authority-keypair.json",
    "airdrop:onchain": "solana airdrop 5 keypairs/on_chain_feed_parser-keypair.json",
    "start": "ts-node src/main.ts",
    "build": "npm-run-all build:*",
    "build:anchor": "cargo build-bpf --manifest-path=rust/anchor-feed-parser/programs/anchor-feed-parser/Cargo.toml && $HOME/.cargo/bin/anchor idl parse -f rust/anchor-feed-parser/programs/anchor-feed-parser/src/lib.rs -o rust/anchor-feed-parser/target/idl/anchor_feed_parser.json",
    "build:onchain": "cargo build-bpf --manifest-path=rust/on-chain-feed-parser/Cargo.toml",
    "deploy": "run-s deploy:*",
    "deploy:anchor": "solana program deploy rust/anchor-feed-parser/target/deploy/on_chain_feed_parser.so --program-id=keypairs/anchor_feed_parser-keypair.json",
    "deploy:onchain": "solana program deploy rust/on-chain-feed-parser/target/deploy/on_chain_feed_parser.so --program-id=keypairs/on_chain_feed_parser-keypair.json",
    "test:anchor": "ts-mocha ./rust/anchor-feed-parser/tests/**/*.ts",
    "test:anchor1": "cd ./rust/anchor-feed-parser && anchor test",
    "test:onchain": "ts-node ./rust/on-chain-feed-parser/tests/on-chain-feed-parser.ts",
    "lint": "eslint --ext .js,.json,.ts 'src/**' --fix",
    "purge": "shx rm -rf {.,rust/*/*}/{node_modules,yarn.lock}",
    "which": "which anchor"
  },
  "keywords": [
    "switchboard"
  ],
  "dependencies": {
    "@project-serum/anchor": "^0.18.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.30.2",
    "@switchboard-xyz/switchboard-api": "^0.2.152",
    "@switchboard-xyz/switchboard-v2": "github:switchboard-xyz/switchboardv2-api",
    "big.js": "^6.1.1",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "node-fetch": "^2.0.0",
    "prompts": "^2.4.2",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13",
    "resolve-dir": "^1.0.1",
    "set-interval-async": "^2.0.3",
    "uuid": "^8.3.2",
    "wait-for-event": "^2.0.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.2",
    "@types/chai": "^4.2.22",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node-fetch": "^2.0.3",
    "@types/prompts": "^2.0.14",
    "@types/readline-sync": "^1.4.4",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "shx": "^0.3.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
