{
  "name": "switchboard-v2-examples",
  "version": "1.0.0",
  "description": "Sample code for Switchboard V2",
  "license": "MIT",
  "private": true,
  "bin": {
    "sbv2-example": "./bin/ts/main.js"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "setup": "npm-run-all -l -c setup:onchain setup:anchor",
    "setup:authority": "run-s create:authority airdrop:authority",
    "setup:anchor": "run-s build:anchor deploy:anchor",
    "setup:onchain": "run-s build:onchain deploy:onchain",
    "create": "run-s create:*",
    "create:authority": "shx find secrets/authority-keypair.json || solana-keygen new -s --no-bip39-passphrase --outfile secrets/authority-keypair.json",
    "airdrop": "run-s airdrop:*",
    "airdrop:authority": "solana airdrop 1.5 secrets/authority-keypair.json && solana airdrop 1.5 secrets/authority-keypair.json || true",
    "build": "run-s build:*",
    "build:anchor": "cd rust/anchor-feed-parser && ${HOME}/.cargo/bin/anchor build",
    "build:onchain": "cargo build-bpf --manifest-path=rust/on-chain-feed-parser/Cargo.toml",
    "build:node": "tsc",
    "deploy": "run-s deploy:*",
    "deploy:anchor": "cd rust/anchor-feed-parser && ${HOME}/.cargo/bin/anchor deploy",
    "deploy:onchain": "solana program deploy rust/on-chain-feed-parser/target/deploy/on_chain_feed_parser.so -k secrets/authority-keypair.json",
    "start": "ts-node ts/main.ts",
    "test": "run-s test:*",
    "test:anchor": "cd rust/anchor-feed-parser && ${HOME}/.cargo/bin/anchor test",
    "test:onchain": "ts-node ./rust/on-chain-feed-parser/tests/on-chain-feed-parser.ts",
    "lint": "eslint --ext .js,.json,.ts 'src/**' --fix",
    "purge": "run-s purge:anchor purge:onchain purge:npm",
    "purge:anchor": "shx rm -rf ./rust/anchor-feed-parser/{target,Cargo.lock,programs/*/target}",
    "purge:onchain": "shx rm -rf ./rust/on-chain-feed-parser/{target,Cargo.lock}",
    "purge:npm": "shx rm -rf node_modules"
  },
  "keywords": [
    "switchboard"
  ],
  "dependencies": {
    "@project-serum/anchor": "^0.23.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.37.1",
    "@switchboard-xyz/switchboard-api": "^0.2.191",
    "@switchboard-xyz/switchboard-v2": "^0.0.72",
    "big.js": "^6.1.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "dotenv": "^10.0.0",
    "find-git-root": "^1.0.4",
    "fs": "^0.0.1-security",
    "mocha": "^9.1.3",
    "node-fetch": "^2.0.0",
    "prompts": "^2.4.2",
    "readline-sync": "^1.4.10",
    "reflect-metadata": "^0.1.13",
    "resolve-dir": "^1.0.1",
    "set-interval-async": "^2.0.3",
    "uuid": "^8.3.2",
    "wait-for-event": "^2.0.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/big.js": "^6.1.2",
    "@types/chai": "^4.2.22",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@types/node-fetch": "^2.0.3",
    "@types/prompts": "^2.0.14",
    "@types/readline-sync": "^1.4.4",
    "@types/yargs": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "shx": "^0.3.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
